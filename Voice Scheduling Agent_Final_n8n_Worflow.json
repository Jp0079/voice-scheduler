      "id": "a478087f-fcb6-4a27-8f90-a1225a9ef1e0",
      "name": "Format Check Availability Response"
    },
    {
      "parameters": {
        "jsCode": "// 1. Get the Tool Call ID safely from the Webhook\nconst webhookData = $('Webhook').first().json.body;\nconst toolCallId = webhookData.message.toolCalls[0].id;\n\n// 2. Get the arguments to make the response personal\nconst args = webhookData.message.toolCalls[0].function.arguments;\n\n// 3. Create a success message\nconst resultText = `Success. I have booked the meeting \"${args.meetingTitle}\" for ${args.customerName}. I have sent the invite to your calendar.`;\n\nreturn {\n    json: {\n        results: [\n            {\n                toolCallId: toolCallId,\n                result: resultText\n            }\n        ]\n    }\n};"
      },
